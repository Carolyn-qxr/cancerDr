<view class="title">
    <image src="{{homeSrc}}" class="home" bindtap="toHome"></image>
<text class="title-text">癌症检测历史</text>
</view>

<view class="body">
    <image class="doctor" src="{{doctorSrc}}"></image>
    <text class="text">您的健康医生</text>
    <view class="container">
        <view class="bigcontainer">
            <view class="shu-xian"></view>
            <view class="line"></view>
            <view class="time">检测时间</view>
            <view class="result">检测结果</view>
            <scroll-view class="context"  scroll-y="true" scroll-with-animation="true">
                <view wx:for="{{array}}">
                    <view class="context-text">
                        <text class="dataTime">{{array[index].dataTime}}</text>
                        <text class="result-text">{{array[index].result}}</text>
                    </view>
                </view>
            </scroll-view>
        </view>
    </view>
    
</view>
